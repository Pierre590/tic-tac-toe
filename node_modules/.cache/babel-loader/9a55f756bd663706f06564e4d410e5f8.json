                 v-for=\"(col, ci) in row\"\n                    :key=\"ci\"\n                    v-on:click= \"addSymbol (ri,ci)\"\n                    class=\"col\">\n                    {{col}}\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Vue from 'vue'\n\nexport default {\n    name: 'app',\n    data() {\n        return {\n            isPlayer :true,\n            nbCase:3,\n            values: []\n        }\n    },\n\n    methods :{\n        addSymbol(row, col) {\n            Vue.set(this.values [row], col, this.isPlayer ? 'X' : 'O')\n            this.isPlayer = !this.isPlayer\n        },\n    },\n\n    created () {\n        for (let i = 0; i < 3; i++){\n            const row = []\n            this.values.push(row)\n            for (let j = 0; j < 3; j++){\n                row.push(null)\n            }\n        }\n    }\n}\n\n</script>\n\n\n<style>\nbody {\n    margin: 0;\n    background-color:black ;\n}\n\n#app {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    height: 100vh;\n    width: 100vw;\n\n}\n.container {\n    display: flex;\n    flex-direction: column;\n    width: 600px;\n    height: 600px;\n}\n\n.row {\n    display: flex;\n    flex: 0 33.33%;\n    border-width: medium;\n    border-color: rgb(186, 231, 24);\n}\n\n.col {\n    flex: 0 33.33%;\n    height: 100%;\n    border: 1px solid;\n    background-color: rgb(186, 231, 24);\n    font-size: 8rem;\n    text-align: center;\n}\n\n\n</style>\n"],"sourceRoot":"src"}]}